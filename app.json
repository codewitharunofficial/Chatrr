{
  "expo": {
    "name": "chatrr",
    "slug": "whatsapp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/Chatrr.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Chatrr.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "plugins": [
        [
          "expo-document-picker",
          {
            "storagePermission": "Allow Chatrr to access you device storage"
          },
          "expo-image-picker",
          {
            "photosPermission": "The App need access to your media"
          },
          [
            "expo-font",
            {
              "fonts": "node_modules/@expo-google-fonts/inter/Inter_100Thin.ttf"
            }
          ],
          {
            "iCloudContainerEnvironment": "Production"
          }
        ]
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_CONTACTS",
        "READ_MEDIA_IMAGES"
      ],
      "package": "com.codewitharun.chatrr"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "64652f17-8852-41ad-b3ac-3d501ec97aaf"
      }
    }
  }
}
